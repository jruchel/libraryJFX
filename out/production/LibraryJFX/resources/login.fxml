      <ProgressIndicator fx:id="progressIndicator" layoutX="286.0" layoutY="83.0" visible="false" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="335.0" />
