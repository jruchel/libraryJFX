<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserPaneController">
    <children>
        <Button fx:id="logoutButton" layoutX="519.0" layoutY="341.0" mnemonicParsing="false" onAction="#logout" text="Logout" AnchorPane.bottomAnchor="40.0" AnchorPane.rightAnchor="10.0" />
        <Button fx:id="showTransactionButton" layoutX="10.0" layoutY="126.0" mnemonicParsing="false" onAction="#showTransactions" text="Transactions" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="125.0" />
        <Button fx:id="showBooksButton" layoutX="13.0" layoutY="96.0" mnemonicParsing="false" onAction="#showBooks" text="Books" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="95.0" />
        <Label fx:id="motdLabel" layoutX="440.0" layoutY="19.0" text="motd" />
        <fx:include fx:id="booksPane" source="booksPane.fxml" />
        <Label fx:id="usernameLabel" layoutX="10.0" layoutY="11.0" text="Username" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
        <fx:include fx:id="transactionsPane" source="transactionPane.fxml" />
        <fx:include fx:id="refundsPane" source="refundsPane.fxml" />
        <Button fx:id="showRefundsButton" layoutX="11.0" layoutY="157.0" mnemonicParsing="false" onAction="#showRefunds" text="Refunds" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="155.0" />
        <Button fx:id="donateButton" layoutX="11.0" layoutY="188.0" mnemonicParsing="false" onAction="#showDonationPane" text="Donate" />
        <fx:include fx:id="donationPane" source="donations.fxml" />
    </children>
</AnchorPane>
