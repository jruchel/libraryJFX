<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.BooksController"
            id="booksPane" fx:id="booksPane" layoutX="214.0" layoutY="100.0" prefHeight="200.0"
            prefWidth="200.0" visible="false" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="100.0"
            AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="100.0">
        <children>
            <ListView fx:id="rentedListView" layoutX="14.0" layoutY="21.0" prefHeight="200.0" prefWidth="200.0"
                      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="200.0"
                      AnchorPane.topAnchor="50.0"/>
            <ListView fx:id="reservedListView" layoutX="200.0" layoutY="30.0" prefHeight="200.0" prefWidth="200.0"
                      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0"
                      AnchorPane.topAnchor="50.0"/>
            <Label layoutX="6.0" layoutY="6.0" text="Rented books" AnchorPane.leftAnchor="0.0"
                   AnchorPane.topAnchor="0.0"/>
            <Label layoutX="200.0" layoutY="6.0" text="Reserved books" AnchorPane.leftAnchor="200.0"
                   AnchorPane.topAnchor="0.0"/>
            <Button fx:id="reserveButton" layoutX="328.0" layoutY="27.0" mnemonicParsing="false" onAction="#reserve"
                    text="Reserve" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="27.0">
                <padding>
                    <Insets bottom="2.0"/>
                </padding>
            </Button>
            <Button fx:id="cancelReservedButton" layoutX="200.0" layoutY="27.0" mnemonicParsing="false"
                    onAction="#cancelReserved" text="Cancel reservation">
                <padding>
                    <Insets bottom="2.0" right="15.0"/>
                </padding>
            </Button>
        </children>
    </AnchorPane>
