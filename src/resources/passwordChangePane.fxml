<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

    <AnchorPane xmlns="http://javafx.com/javafx"
                xmlns:fx="http://javafx.com/fxml"
                fx:controller="controllers.PasswordChangePaneController"
                fx:id="passwordChangePane" layoutX="114.0" layoutY="110.0" prefHeight="200.0" prefWidth="399.0"
                visible="false" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="100.0"
                AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="100.0">
        <children>
            <Button fx:id="acceptPasswordButton" layoutX="333.0" layoutY="148.0" mnemonicParsing="false"
                    onAction="#changePassword" text="Accept" AnchorPane.bottomAnchor="87.0"
                    AnchorPane.rightAnchor="15.0"/>
            <PasswordField fx:id="currentPasswordField" layoutX="125.0" layoutY="33.0" promptText="Current password"
                           AnchorPane.leftAnchor="125.0" AnchorPane.rightAnchor="125.0"
                           AnchorPane.topAnchor="33.0"/>
            <PasswordField fx:id="newPasswordField" layoutX="125.0" layoutY="65.0" promptText="New password"
                           AnchorPane.bottomAnchor="175.0" AnchorPane.leftAnchor="125.0"
                           AnchorPane.rightAnchor="125.0" AnchorPane.topAnchor="60.0"/>
            <PasswordField fx:id="confirmPasswordField" layoutX="125.0" layoutY="100.0"
                           promptText="Confirm password" AnchorPane.leftAnchor="125.0"
                           AnchorPane.rightAnchor="125.0" AnchorPane.topAnchor="90.0"/>
        </children>
    </AnchorPane>
